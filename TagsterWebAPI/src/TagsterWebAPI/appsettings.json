{
  "swagger": {
    "enabled": false,
    "name": "tagster",
    "title": "tagster",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true,
    "contactName": "support",
    "contactEmail": ""
  },
  "logger": {
    "level": "information",
    "excludePaths": [ "/ping", "/metrics" ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": false
    },
    "file": {
      "enabled": false,
      "Path": "",
      "Interval": "day"
    },
    "elk": {
      "enabled": false,
      "url": "",
      "basicAuthEnabled": false,
      "username": "",
      "password": "",
      "indexFormat": ""
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "apiKey": "secret"
    }
  },

  "AllowedHosts": "*"
}
